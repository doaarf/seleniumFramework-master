<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" parallel="tests" thread-count="2">

	<test name="Chrome Regression Test">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="tests.UserRegistrationParallelTesting" />
		</classes>
	</test>

	<test name="chrome-headless Regression Test">
		<parameter name="browser" value="chrome-headless" />
		<classes>
			<class name="tests.MyAccountPT" />
		</classes>
	</test>
</suite> 
